{
  "name": "administrate-lms",
  "main": "./src/App",
  "private": true,
  "scripts": {
    "postinstall": "cd distribution && npm install",
    "types:lms": "cd dx/graphql && npm run update:lms -- --watch",
    "voyager": "cd dx/graphql && npm run voyager",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "preexport": "rimraf ./dist",
    "export": "expo export --public-url https://administrate-lms.glenstack.workers.dev/",
    "predeploy": "npm run export",
    "deploy": "cd distribution && npm run deploy"
  },
  "dependencies": {
